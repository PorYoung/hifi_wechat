                            $('.logoQRShow').parent().parent().removeAttr('style').parent().css({
                                display:'flex',
                                'justify-content':'center',
                                'align-items':'center'
                            })
                            $('.logoQRShow').parents('#myLoadingWrap').on('click',function(){
                                clearAlertGuestsInfoPanel()
                                setGuestsTimeoutRotate()
                            })
                        })
                    config.QRSrc = data
                }
            })
        })
        
    </script>
